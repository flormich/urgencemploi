{% extends 'base.html.twig' %}

{% block title %}UrgencEmploi{% endblock %}

{% block main %}
    {% include 'baseOpen.html.php' %}
    <p class="h2 col-12">
        <br><br><br>
        <section class="paragraphe subtitle">
            {% if app.session.flashBag.has('action') %}
                <div class="red alert-success">
                    {% for msg in app.session.flashBag.get('action') %}
                        <h3>{{ msg }}</h3>
                    {% endfor %}
                </div>
            {% endif %}
            {# <meta http-equiv="refresh" content="1; url=index" /> #}
            <form action="{{ path('index') }}">
                <button class="mdl-button mdl-js-button mdl-button--raised mdl-button--myColorValidate" type="submit">
                    <a href="index" style="color:white; text-decoration:none">{{ 'button.index'|trans }}</a>
                </button>
            </form>
        </section>
    </p>
{% endblock %}
        